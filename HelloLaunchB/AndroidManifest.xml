<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hellolaunchb"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.hellolaunchb.Main"
            android:launchMode="singleInstance" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.hellolaunchb.Standard"
            android:launchMode="standard"
            android:taskAffinity="com.example.myhellodx" />
        <activity
            android:name="com.example.hellolaunchb.SingleTop"
            android:launchMode="singleTop" />
        <activity
            android:name="com.example.hellolaunchb.SingleTask"
            android:launchMode="singleTask" />
        <activity
            android:name="com.example.hellolaunchb.SingleInstance"
            android:launchMode="singleInstance" />
    </application>

</manifest>